{
	"name": "wraiths",
	"version": "1.0.0",
	"authors": [
		"Fabio Cencetti"
	],
	"description": "BEMIT framework",
	"main": [
		"./wraiths/styles/app.scss",
		"./wraiths/scripts/app.js"
	],
	"moduleType": [
		"amd"
	],
	"keywords": [
		"wraiths"
	],
	"license": "MIT",
	"homepage": "",
	"private": false,
	"ignore": [
		"**/.*",
		"node_modules",
		"bower_components",
		"*.map",
		"*.json",
		"*.md",
