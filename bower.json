		"sass-mediaqueries": "~1.6.1",
		"neat": "~1.7.2"
	},
	"devDependencies": {},
	"overrides": {},
	"install": {
		"base": "wraiths",
		"path": "vendor",
		"sources": {},
		"ignore": [
			"requirejs",
			"gsap",
			"emitter-es6",
			"bourbon",
			"neat",
			"sass-mediaqueries",
			"sassy-gridlover"
